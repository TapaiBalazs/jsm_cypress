{
  "npmScope": "cat",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-interfaces": {
      "tags": []
    },
    "app-e2e": {
      "tags": [],
      "implicitDependencies": ["customer", "api", "backoffice"]
    },
    "backoffice": {
      "tags": []
    },
    "backoffice-functional": {
      "tags": [],
      "implicitDependencies": ["backoffice"]
    },
    "customer": {
      "tags": []
    },
    "customer-functional": {
      "tags": [],
      "implicitDependencies": ["customer"]
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  }
}
