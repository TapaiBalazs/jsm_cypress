{
  "name": "cat",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "start:customer": "npm run start customer",
    "start:backoffice": "npm run start backoffice",
    "start:api": "npm run start api",
    "start:all": "concurrently -k -n \"CUSTOMER,BACKOFFICE,API\" -c \"yellow,blue,cyan\" \"npm run start:customer\" \"npm run start:backoffice\" \"npm run start:api\"",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "e2e:system": "npm run e2e app-e2e -- --headless",
    "e2e:system:debug": "npm run e2e app-e2e -- --watch",
    "functional:customer": "npm run e2e customer-functional -- --headless",
    "functional:customer:debug": "npm run e2e customer-functional -- --watch",
    "functional:backoffice": "npm run e2e backoffice-functional -- --headless",
    "functional:backoffice:debug": "npm run e2e backoffice-functional -- --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "snapshot:customer-functional": "npm run e2e -- --skip-nx-cache --configuration=snapshot customer-functional",
    "snapshot:customer-functional:docker": "docker run -it --rm -e CYPRESS_updateSnapshots=%CYPRESS_updateSnapshots% -v $PWD:/cypress -w /cypress snapshot-testing",
    "snapshot:customer-functional:docker:windows": "docker run -it --rm -e CYPRESS_updateSnapshots=%CYPRESS_updateSnapshots% -v %cd%:/cypress -w /cypress snapshot-testing",
    "snapshot:customer-functional:update-snapshots": "CYPRESS_updateSnapshots=true npm run affected:e2e:snapshot",
    "snapshot:customer-functional:update-snapshots:windows": "set CYPRESS_updateSnapshots=true && npm run affected:e2e:snapshot",
    "devtools:build:docker": "docker build . -f tools/snapshot-comparison/Dockerfile -t snapshot-testing",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "cypress:setup": "cypress install && cypress verify"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.4",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "12.1.4",
    "@angular/compiler": "12.1.4",
    "@angular/core": "12.1.4",
    "@angular/forms": "12.1.4",
    "@angular/material": "12.1.4",
    "@angular/platform-browser": "12.1.4",
    "@angular/platform-browser-dynamic": "12.1.4",
    "@angular/router": "12.1.4",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.7.13",
    "@nrwl/angular": "12.6.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.4",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.1.4",
    "@angular/compiler-cli": "12.1.4",
    "@angular/language-service": "12.1.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "12.6.2",
    "@nrwl/cypress": "12.6.2",
    "@nrwl/eslint-plugin-nx": "12.6.2",
    "@nrwl/jest": "12.6.2",
    "@nrwl/linter": "12.6.2",
    "@nrwl/nest": "12.6.2",
    "@nrwl/node": "12.6.2",
    "@nrwl/tao": "12.6.2",
    "@nrwl/workspace": "12.6.2",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/express": "^4.17.11",
    "@types/jest": "26.0.8",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "concurrently": "^6.0.0",
    "cypress": "^8.0.0",
    "cypress-image-snapshot": "^4.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.3",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "ts-node": "~9.1.1",
    "typescript": "4.3.5"
  }
}
